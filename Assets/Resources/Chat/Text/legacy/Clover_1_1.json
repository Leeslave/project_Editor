        {
            "$type": "ChoiceParagraph, Assembly-CSharp",
            "choices": [
                {
                    "text": "감기...?"
                },
                {
                    "text": "많이 아프대?"
                }
            ],
            "type":1
        },
        {
            "$type": "TalkParagraph, Assembly-CSharp",
            "talker": "클로버",
            "text":"엣? 아뇨" 
        },
        {
            "$type": "TalkParagraph, Assembly-CSharp",
            "talker": "클로버",
            "talkerInfo": "렉스 따라하기",
            "text":"아잇, 괜찮아요. 집에서 하루 쉬면 말짱해질 겁니다 헤헤" 
        },
        {
            "$type": "TalkParagraph, Assembly-CSharp",
            "talker": "클로버",
            "text":"라고 하던데요~?" 
        },
        {
            "$type": "TalkParagraph, Assembly-CSharp",
