            } ,
            "characterR": {
                "fileName": "Handerson",
                "index": 3
            } 
        },
        {
            "$type": "TalkParagraph, Assembly-CSharp",
            "talker": "핸더슨",
            "text":"괜히 한다고 했어.",
            "characterL": {
