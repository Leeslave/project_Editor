        },
        {
            "$type": "TalkParagraph, Assembly-CSharp",
            "talker": "클레이튼",
            "text":"겨울이니까.",
            "characterL": {
                "fileName": "Klayton",
                "index": 0
            },
            "characterR": {
                "fileName": "price",
                "index": 1
