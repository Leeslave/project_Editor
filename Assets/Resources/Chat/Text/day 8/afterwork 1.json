            "characterR": {
                "fileName": "price",
                "index": 1
            } 
        },
        {
            "$type": "TalkParagraph, Assembly-CSharp",
            "talker": "클레이튼",
            "text":"난 내가 알아서 하니, 자네나 자네 앞길을 걱정하게.",
